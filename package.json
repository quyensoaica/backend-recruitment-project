{
  "name": "backend-recruiment-project",
  "version": "1.0.0",
  "description": "backend recruiment project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src -d ./build/src",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && node ./build/src/index.js",
    "start": "nodemon -r tsconfig-paths/register ./src/index.ts",
    "dev": "nodemon -r tsconfig-paths/register ./src/index.ts",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:rollback": "npx sequelize-cli db:migrate:undo",
    "db:create": "npx sequelize-cli create"
  },
  "keywords": [
    "recruiment"
  ],
  "author": "quyensoaica",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.25.4",
    "@sequelize/postgres": "^7.0.0-alpha.41",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.0",
    "@types/sequelize": "^4.28.20",
    "async-exit-hook": "^2.0.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.3",
    "mongodb": "^6.8.0",
    "mongoose": "^8.5.4",
    "nodemon": "^3.1.4",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/node": "^7.25.0",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@types/async-exit-hook": "^2.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "eslint": "^9.9.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}
